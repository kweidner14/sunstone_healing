{
  "name": "emily-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/stylesheets/style.css -w",
    "serve": "live-server",
    "dev": "node ./bin/www",
    "begin": "npm-run-all --parallel serve watch:sass",
    "devserver": "npm run dev | npm run begin",
    "start": "node ./bin/www",
    "compile:sass": "node-sass sass/main.scss public/stylesheets/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/stylesheets/style.comp.css -o public/stylesheets/style.prefix.css",
    "compress:css": "node-sass public/stylesheets/style.prefix.css public/stylesheets/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Kyle Weidner",
  "dependencies": {
    "async": "^3.2.2",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "cookie-parser": "~1.4.4",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "ejs-mate": "^3.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.20",
    "mongoose-paginate": "^5.0.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-sass-middleware": "0.11.0",
    "nodemailer": "^6.3.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "ssl-express-www": "^3.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  },
  "engines": {
    "node": "12.4.0",
    "npm": "6.14.4"
  }
}
